{
    "manifestVersion": 1,
    "id": "BeagleSecurity",
    "name": "Beagle-Security-Task",
    "version": "1.0.7",
    "publisher": "BeagleSecurity",
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "public": true,
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Trigger beagle security test",
    "categories": [
        "Azure Pipelines"
    ],
    "icons": {
        "default": "img/icon.png"        
    },
    "files": [
        {
            "path": "beagle-azure-task"
        }
    ],
    "contributions": [
        {
            "id": "beagle-start-test-azure-task",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "beagle-azure-task"
            }
        }
    ]
}